            </Button>
            <Button mnemonicParsing="false" onAction="#logInOperation" prefHeight="25.0" prefWidth="350.0" styleClass="buttons2" text="Save Setting">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </VBox>
   </children>
