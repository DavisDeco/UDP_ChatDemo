<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="233.0" styleClass="gridBackground" stylesheets="@../chat.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="daviscahtech_chatdemo.Controllers.mainPageController">
