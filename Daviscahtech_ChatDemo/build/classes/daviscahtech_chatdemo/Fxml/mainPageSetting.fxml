         <children>
            <TextField fx:id="serverName" focusTraversable="false" promptText="Username" styleClass="fields">
               <padding>
                  <Insets bottom="5.0" left="20.0" top="5.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="userPassword" focusTraversable="false" promptText="password" styleClass="fields">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="20.0" top="5.0" />
               </padding>
            </PasswordField>
            <Separator prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Separator>
            <TextField fx:id="ipAddress" focusTraversable="false" layoutX="10.0" layoutY="198.0" promptText="IP Address" styleClass="fields">
