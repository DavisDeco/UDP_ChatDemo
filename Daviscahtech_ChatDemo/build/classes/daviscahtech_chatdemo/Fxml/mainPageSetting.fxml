               </padding>
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#updateSettingOperation" prefHeight="25.0" prefWidth="261.0" styleClass="buttons2" text="Update detail">
